<%= simple_form_for(@run) do |r| %>
  <%= r.input :date %>
  <%= r.input :location %>
  <%= r.input :distance %>
  <%= r.input :description %>
  <%= r.input :capacity %>
  <%= r.input :photo %>
  <%= r.input :photo_cache, as: :hidden %>
  <%= r.submit "Submit", class: "btn btn-default" %>
<% end %>
